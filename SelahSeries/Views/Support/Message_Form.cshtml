<div class="site-blocks-cover inner-page-cover overlay" style="background-image: url(/NewFolder2/background/alex-holyoake-6RcBHxfPhL0-unsplash.jpg)" data-aos="fade" data-stellar-background-ratio="0.5" data-aos="fade">
        <div class="container">
            <div class="row align-items-center justify-content-center">
                <div class="col-md-7 text-center" data-aos="fade-up" data-aos-delay="400">
                    <h2 class="support_head">Send Us A Message</h2>
                </div>
            </div>
        </div>
    </div>

<div class="container">
    <div class="featured">
        
                <div class="card card-2">
                    <div class="card-heading"></div>
                    <div class="card-body">
                        <h4 class="title" style="color:black" ; text-transform:"inherit">Are you in need of counselling/prayer? Contact our support links and we will get in touch with you.</h4>
                        <form method="POST">
                            <div class="input-group">
                                <input class="input--style-2" type="text" placeholder="Full name" name="fullname" required="required">
                            </div>
                            <div class="row row-space">
                                <div class="col-2">
                                    <div class="input-group">
                                        <input class="input--style-2" type="number" placeholder="Age" name="age" required="required">
                                    </div>
                                </div>
                                <div class="col-2">
                                    <div class="input-group">
                                        <div class="rs-select2 js-select-simple select--no-search">
                                            <select name="gender" required="required">
                                                <option disabled="disabled" selected="selected">Gender</option>
                                                <option>Male</option>
                                                <option>Female</option>
                                            </select>
                                            <div class="select-dropdown"></div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="row row-space">
                                <div class="col-2">
                                    <div class="input-group">
                                        <input class="input--style-2" type="number" placeholder="Phone Number" name="phone" required="required">
                                    </div>
                                </div>
                                <div class="col-2">
                                    <div class="input-group">
                                        <div class="rs-select2 js-select-simple select--no-search">
                                            <select name="category" required="required">
                                                <option disabled="disabled" selected="selected">Message Category</option>
                                                <option>Prayer</option>
                                                <option>Counselling</option>
                                            </select>
                                            <div class="select-dropdown"></div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="input-group">
                                <input class="input--style-2" type="email" placeholder="Email" name="email" required="required">
                            </div>
                            <div class="input-group">
                                <input class="input--style-2" type="text" placeholder="Your address" name="address" required="required">
                            </div>
                            <div class="input-group">
                                <textarea class="input--style-2" name="message" placeholder="Write us a message" required="required"></textarea>
                            </div>

                            <div class="p-t-30">
                                <button class="btn btn--radius btn--green" type="submit">Send</button>
                            </div>
                        </form>
                    </div>
                </div>
           
    </div>
</div>

