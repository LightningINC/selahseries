    }
    .navbar{
        padding-left: 7%;
        padding-right: 7%;
    }
    .header{
        margin-bottom: 0px;
    }
    .dropdown-menu a:hover {
        color: rgba(0, 123, 255,0.6);
    }
    .navbar-light .navbar-nav .active > .nav-link{
        color: #ad001c;
    }
    .header{
        border-bottom: 1px solid #007bff;
    }
    .navbar-light .navbar-nav .nav-link {
        color: rgba(0,0,0,.9);
    }
</style>
