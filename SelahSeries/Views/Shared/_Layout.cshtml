                                Blog
                            </a>
                            <div class="dropdown-menu" aria-labelledby="navbarDropdownMenuLink">
                                <a class="dropdown-item" href="#">Relationship & Marriage</a>
                                <a class="dropdown-item" href="#">Politics</a>
                                <a class="dropdown-item" href="#">Sport</a>
                            </div>
                        </li>
                        <li class="menu_mm"><a href="#">Support/Counselling</a></li>
                        <li class="menu_mm"><a href="#">Books</a></li>
                        <li class="menu_mm"><a href="#">About</a></li>
                        <li class="menu_mm"><a href="#">Contact</a></li>
                    </ul>
                </nav>
            </div>*@

        }


        @RenderBody()

        @if (ViewData["Add Navigation"] != "false")
        {
            <!-- Footer -->

            <footer class="footer">
                <div class="container">
                    <div class="row row-lg-eq-height">
                        <div class="col-lg-12 order-lg-1 order-2">
                            <div class="footer_content">
                                <div class="footer_logo" style="color: #f2ba16"><a href="#">selah</a></div>
                                <div class="footer_social">
                                    <ul>
                                        <li class="footer_social_facebook"><a href="#"><i class="fa fa-facebook" aria-hidden="true"></i></a></li>
