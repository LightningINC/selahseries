                                    <li class="post_share_item">
                                        <a href="https://facebook.com/sharer/sharer.php?u=@Uri.EscapeDataString(pageURL)&t=@Model.Post.Title"
                                           class="share-popup"
                                           target="_blank"><i class="fa fa-facebook" aria-hidden="true"></i></a>
                                    </li>
                                    <li class="post_share_item">
                                        <a href="https://twitter.com/share?url=@Uri.EscapeDataString(pageURL)&via=2uung&text=@Model.Post.Title"
                                           class="share-popup"
